<?xml version="1.0" encoding="utf-8" ?>
<configuration>
 <configSections>
	<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
 </configSections>
 <appSettings>
  <clear />
 </appSettings>
 <log4net>
	<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender" >
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date %-5level %logger - %message%newline" />
		</layout>
 	</appender>
	<appender name="FileAppender" type="TickZoom.Logging.FileAppender" >
		<file value="LogFolder\IBProviderService.log" />
		<appendToFile value="true" />
		<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date %-5level %logger - %message%newline" />
		</layout>
 	</appender>
	<root>
		<level value="INFO" />
		<appender-ref ref="FileAppender" />
	</root>
    <logger name="TickZoom.Common.LogicalOrderHandlerDefault">
        <level value="DEBUG" />
    </logger>
    <logger name="TickZoom.InteractiveBrokers">
        <level value="DEBUG" />
    </logger>
    <logger name="TickZoom.ProviderService">
        <level value="INFO" />
    </logger>
    <logger name="TickZoom.SocketAPI">
        <level value="INFO" />
    </logger>
 </log4net>
</configuration>